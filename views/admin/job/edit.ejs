<%- include('../navbar.ejs') %>
    <%- include('../sidebar.ejs') %>


        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <div class="content-header">
                <div class="container-fluid">
                    <div class="row mb-2">
                        <div class="col-sm-6">
                            <h1 class="m-0 text-dark">Job Edit</h1>
                        </div><!-- /.col -->
                        <div class="col-sm-6">
                            <ol class="breadcrumb float-sm-right">
                                <li class="breadcrumb-item"><a href="#">Job</a></li>
                                <li class="breadcrumb-item active">Job Edit</li>
                            </ol>
                        </div><!-- /.col -->
                    </div><!-- /.row -->
                </div><!-- /.container-fluid -->
            </div>
            <!-- /.content-header -->

            <!-- Main content -->
            <div class="content">
                <div class="container-fluid">
                    <div class="card">
                        <div class="card-header">
                        </div>
                        <!-- /.card-header -->
                        <div class="card-body">
                            <form action="/updatejob/<%= j._id %>" method="post">
                                <div class="form-group">
                                    <label for="">Title</label>
                                    <input type="text" value="<%=j.title %>" class="form-control" name="title"
                                        required>
                                    <label for="">Description</label>
                                    <input type="text" value="<%=j.title %>" class="form-control" name="title"
                                        required>
                                    <label for="">Location</label>
                                    <input type="text" value="<%=j.title %>" class="form-control" name="title"
                                        required>
                                    <label for="">CompanyName</label>
                                    <input type="text" value="<%=j.title %>" class="form-control" name="title"
                                        required>
                                    <label for="">JobType</label>
                                    <input type="text" value="<%=j.title %>" class="form-control" name="title"
                                        required>
                                    <label for="">SalaryRange</label>
                                    <input type="text" value="<%=j.title %>" class="form-control" name="title"
                                        required>
                                    <label for="">skillsRequired</label>
                                    <input type="text" value="<%=j.title %>" class="form-control" name="title"
                                        required>
                                </div>
                                <button type="submit" class="btn btn-info">Update</button>
                            </form>



                        </div>
                        <!-- /.card-body -->
                    </div>
                    <!-- /.card -->


                </div><!-- /.container-fluid -->
            </div>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->



